
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Data {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  date      DateTime
  age       String 
  gender    String 
  a         Int
  b         Int
  c         Int
  d         Int
  e         Int
  f         Int
}


model User {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  email       String   @unique
  password    String
}